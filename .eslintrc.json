{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "rules": {
    },
    "overrides": [
        {
            "files": [
                "src/**/*.ts"
            ],
            "extends": [
                "airbnb-base",
                "airbnb-typescript/base",
                "p5js"
            ],
            "parser": "@typescript-eslint/parser",
            "plugins": [
                "@typescript-eslint"
            ],
            "rules": {
                "no-console": "off",
                "import/no-cycle": "off"
            }
        }
    ]
}
